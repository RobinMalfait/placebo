{
  "name": "placebo",
  "version": "0.0.0",
  "description": "Placebo, the beautiful diagnostics printer",
  "main": "src/index.js",
  "scripts": {
    "format": "prettier --write .",
    "test": "PLACEBO_PRINT_WIDTH=100 jest",
    "examples:generate-readme": "PLACEBO_PRINT_WIDTH=100 node ./scripts/generate-example-readmes.js"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [
    "diagnostics"
  ],
  "author": "Robin Malfait",
  "license": "MIT",
  "devDependencies": {
    "jest": "^28.1.3",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "cli-highlight": "^2.1.11",
    "kleur": "^4.1.4"
  }
}
